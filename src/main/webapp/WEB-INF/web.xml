<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>
            com.varentech.deploya.Form.LoginServlet
        </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>formServlet</servlet-name>
        <servlet-class>
            com.varentech.deploya.Form.FormServlet
        </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>formServlet</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
</web-app>


